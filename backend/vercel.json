{
  "version": 2,
  "name": "project-backend",
  "builds": [
    {
      "src": "api/favorites.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "backend/serviceAccountKey.json"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/favorites.js"
    }
  ],
  "crons": [
    {
      "path": "/api/favorites",
      "schedule": "0 10 * * *"  
    }
  ]
}
